<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
                   http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <changeSet id="2020-05-22-add-subject-type" author="leonid">
        <addColumn tableName="test_subject">
            <column name="subject_type" type="${string.type}" />
        </addColumn>
    </changeSet>
    <changeSet id="2020-05-22-populate-subject-type" author="leonid">
        <update tableName="test_subject">
            <column name="subject_type" value="UNDEFINED"/>
        </update>
    </changeSet>

</databaseChangeLog>