      xmlns:p="tapestry:parameter"><!-- Тестирование -->

<div class="row">
    <h2>${message:testing-header} ${testingPlan.subject.title} (${testingPlan.name})</h2>
</div>

<div class="row" id="questionView">
    <div class="col-sm-8 col-sm-offset-2 text-lg">${startTestMessage}</div>
</div>

<div class="row subquestion-nav">
    <div class="col-sm-8 col-sm-offset-2"><t:pagelink page="testwizard" class="btn btn-primary">${message:start-test}</t:pagelink> </div>
</div>
</html>