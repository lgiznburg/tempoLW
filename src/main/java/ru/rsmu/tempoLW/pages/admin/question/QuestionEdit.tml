<html t:type="layout" t:pageTitle="${message:questionedit-pagetitle}"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

<div class="row">
    <h2>${question.questionInfo.subject.title} : ${message:edit} ${message:question} </h2>
</div>

<div>

        <t:delegate to="questionBlock"/>

</div>

<t:block t:id="questionSimple"><t:admin.question.questionsimpleedit question="question"/></t:block>
<t:block t:id="questionOpen"><t:admin.question.questionopenedit question="question"/></t:block>
<t:block t:id="questionSimpleOrder"><t:admin.question.questionsimpleorderedit question="question"/></t:block>
<t:block t:id="questionCorrespondence"><t:admin.question.questioncorrespondenceedit question="question"/></t:block>
<t:block t:id="questionTree"><t:admin.question.questiontreeedit question="question"/></t:block>
<t:block t:id="questionFree"><t:admin.question.questionfreeedit question="question"/></t:block>
<t:block t:id="questionTreeOpen"><t:admin.question.questiontreeopenedit question="question"/></t:block>
<t:block t:id="questionCrossword"><t:admin.question.questioncrosswordedit question="question"/></t:block>
<t:block t:id="questionBigOpen"> <t:admin.question.questionbigopenedit question="question"/></t:block>

</html>